<h1>タイトル一覧</h1>

<%= link_to "新規作成", :new_article %>

<% @articles.each do |article| %>
<ul>
  <li><%= article.name  %></li>
  <li><%= article.title  %></li>
  <li><%= article.content  %></li>
  <li><%= article.created_at %></li>
</ul>

<%= link_to "編集", [:edit, article] %> | <%= link_to "削除", article, method: :delete, confirm: "本当に削除しますか？" %>
<% end %>

<%= session[:user_id] %>


